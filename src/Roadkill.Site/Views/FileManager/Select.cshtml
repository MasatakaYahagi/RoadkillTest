@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
		@Scripts.Render("~/Assets/Scripts/roadkill.js")
		@Styles.Render("~/Assets/CSS/bundle.css")
		@Url.BootStrap()
		<link href="@Url.Content(SiteSettings.ThemePath + "/Theme.css")" rel="stylesheet" type="text/css" />
		<link href="@Url.Content(SiteSettings.ThemePath + "/Theme.print.css")" rel="stylesheet" type="text/css" media="print" />
		<script type="text/javascript" src="@Url.Action("GlobalJsVars","Home")"></script>
		<link rel="shortcut icon" href="@Url.Content("~/Assets/Images/favicon.png")" />
</head>
<body>
    <style> body { background-color: whitesmoke; } </style>
    @Html.Partial("Navigator")
    <script type="text/javascript">
    	$(function ()
    	{
    		Roadkill.Site.FileManager.Setup.configure();
    		$("table#files").live("fileselected",
                function (e, data)
                {
                	window.top.addImage(data.file);
                });
    	});
    </script>
</body>
</html>