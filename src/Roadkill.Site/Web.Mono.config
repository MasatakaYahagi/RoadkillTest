<?xml version="1.0"?>

<!-- This config is the same as the download config -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<roadkill adminRoleName="Admin"
			attachmentsFolder="~/App_Data/Attachments"
			useObjectCache="true"
			useBrowserCache="false"
			connectionStringName="Roadkill"
			dataStoreType="SqlServer2005"
			editorRoleName="Editor"
			logging="none"
			logErrorsOnly="true"
			installed="false"
			useWindowsAuthentication="false"
			version="1.6.0"
			xdt:Transform="Replace" />

	<system.web>
		<customErrors mode="On" xdt:Transform="Replace"/>
	</system.web>

	<connectionStrings xdt:Transform="Replace">
		<add name="Roadkill" connectionString="" />
	</connectionStrings>
</configuration>